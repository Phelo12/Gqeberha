@model IEnumerable<GqeberhaClinic.Models.Vaccine_Education>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Walk-In.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VaccineName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.VaccineDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Purpose)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Image)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VaccineName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VaccineDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Purpose)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Image)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.EducationID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.EducationID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.EducationID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
